# 位置：frontend/.env.example
REACT_APP_API_URL=http://localhost:5004/api
REACT_APP_WS_URL=ws://localhost:5004/ws

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id

# Environment
REACT_APP_ENV=development

# CORS Configuration
REACT_APP_ALLOW_CREDENTIALS=true
REACT_APP_ALLOWED_ORIGINS=http://localhost:3000

# API Timeout
REACT_APP_API_TIMEOUT=30000

# Debug Mode
REACT_APP_DEBUG=true

# Port Configuration
PORT=3000

# Source Map
GENERATE_SOURCEMAP=false 